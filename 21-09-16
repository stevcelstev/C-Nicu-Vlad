using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication5
{
    class Program
    {
        // SUBPROGRAM 
        static void AfisareInterval(short inf, short sup)
        {
            short i;
            for (i = inf; i <= sup; i++)
            {
                Console.Write(i + " ");
            }
            Console.WriteLine();
        }
        
        static void Main(string[] args)
            AfisareInterval(1, 5);
        }
        
        static void Main(string[] args)      
        {
        // SUMA UNUI VECTOR
            int[] x;
            int n, i, s = 0;
            Console.Write("NUMARUL DE ELEMENTE: ");
            n = int.Parse(Console.ReadLine());

            x = new int[n];
            for (i = 0; i < x.Length; i++)
            {
                Console.Write("x[" + (i + 1) + "]= ");
                x[i] = int.Parse(Console.ReadLine());
            }

            for (i = 0; i < x.Length; i++)
            {
                s += x[i];
            }
            Console.Write("SUMA ELEMENTELOR ESTE: " + s);
            Console.ReadLine();
        }
    }
}
